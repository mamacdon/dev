# Git aliases
alias ga="git add"
alias gcach="git commit --amend -C HEAD"
alias gb="git branch"
alias gbug=git_bug
alias gd="git diff"
alias gds="git diff --staged"
alias gl="git log"
alias gk=call_gitk
alias glt="git glt"
alias glm="git glm"
alias glp="git log --pretty=full"
alias gp="git pull"
alias grbi="git rbi"
alias grc="git rebase --continue"
alias gra="git rebase --abort"
alias gs="git status -sb"
alias gw="git show"
alias gss="git show --stat"
